{
  "name": "reason-react-native-web-example",
  "version": "0.6.3",
  "license": "MIT",
  "scripts": {
    "dev": "npm run js-watch | npm run bsb-watch",
    "js-watch": "razzle start",
    "bsb-watch": "bsb -make-world -w",
    "clean": "bsb -clean-world && rm -rf build",
    "build": "bsb -make-world && razzle build",
    "start": "NODE_ENV=production node build/server.js",
    "format": "find . -name \"*.re\" | grep -v \"node_modules\" | xargs bsrefmt --in-place"
  },
  "dependencies": {
    "bs-react-native": "^0.10.0",
    "compression": "^1.6.2",
    "express": "^4.16.4",
    "helmet": "^3.16.0",
    "hpp": "^0.2.2",
    "morgan": "^1.9.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-native-web": "^0.11.1",
    "reason-react": "^0.5.3"
  },
  "devDependencies": {
    "bs-platform": "^4.0.18",
    "razzle": "^2.4.1"
  }
}
