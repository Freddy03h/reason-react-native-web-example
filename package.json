{
  "name": "reason-react-native-web-example",
  "version": "0.6.3",
  "license": "MIT",
  "scripts": {
    "dev": "npm run js-watch | npm run bsb-watch",
    "js-watch": "razzle start",
    "bsb-watch": "bsb -make-world -w 2>&1 | berror --path-to-refmttype /usr/local/bin/refmt",
    "clean": "bsb -clean-world && rm -rf build",
    "build": "bsb -make-world && razzle build",
    "start": "NODE_ENV=production node build/server.js"
  },
  "dependencies": {
    "bs-react-native": "https://github.com/BuckleTypes/bs-react-native.git",
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "helmet": "^3.6.1",
    "hpp": "^0.2.2",
    "morgan": "^1.8.2",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-native-web": "^0.0.97",
    "react-router-dom": "^4.1.1",
    "reason-react": "^0.1.5"
  },
  "devDependencies": {
    "bs-platform": "^1.7.5",
    "razzle": "latest"
  }
}
